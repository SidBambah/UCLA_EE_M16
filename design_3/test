    %vpi_call 2 179 "$display", "\011Incorrect Match to length: %b to: %b", v0x20e0ba0_0, v0x20e04a0_0;
T_10.7 ;
    %jmp T_10.3;
T_10.2 ;
    %movi 8, 7, 32;
    %load/v 40, v0x20e0680_0, 32;
    %cmp/s 8, 40, 32;
    %or 5, 4, 1;
    %mov 8, 5, 1;
    %load/v 9, v0x20e0680_0, 32;
   %cmpi/s 9, 12, 32;
    %or 5, 4, 1;
    %mov 9, 5, 1;
    %and 8, 9, 1;
    %jmp/0xz  T_10.8, 8;
    %vpi_call 2 183 "$display", "Space Found:";
    %load/v 8, v0x20e04a0_0, 4;
    %cmpi/u 8, 0, 4;
    %jmp/0xz  T_10.10, 4;
    %vpi_call 2 185 "$display", "\011Match space";
    %jmp T_10.11;
T_10.10 ;
    %vpi_call 2 187 "$display", "\011Incorrect Blanks";
T_10.11 ;
    %jmp T_10.9;
T_10.8 ;
    %vpi_call 2 190 "$display", "\011Incorrect Blanks";
T_10.9 ;
T_10.3 ;
    %set/v v0x20e0d90_0, 0, 1;
T_10.0 ;
    %jmp T_10;
    .thread T_10;
